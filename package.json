{
  "name": "@geensnor/astro-componenten",
  "version": "2.1.8",
  "description": "Paar prachtige Astro componenten in Geensnor stijl",
  "homepage": "https://github.com/geensnor/astro-componenten#readme",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./de-snor": "./src/components/de-snor.astro",
    "./kop": "./src/components/kop.astro",
    "./knoepert": "./src/components/knoepert.astro",
    "./lijstitem": "./src/components/lijstitem.astro",
    "./tegel": "./src/components/tegel.astro",
    "./voettekst": "./src/components/voettekst.astro"
  },
  "files": [
    "index.ts",
    "src/*",
    "assets/*",
    "fonts/*"
  ],
  "keywords": [
    "astro-component",
    "geensnor"
  ],
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "format": "npx prettier -w src/*",
    "lint": "eslint --ext .ts,.js,.astro src/*",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/ui": "^3.2.4",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.38.0",
    "eslint-plugin-astro": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.12.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fontsource-variable/work-sans": "^5.2.8",
    "@fontsource/zilla-slab": "^5.2.8",
    "astro": "^5.15.2"
  },
  "peerDependencies": {
    "esbuild": "^0.25.9"
  }
}
