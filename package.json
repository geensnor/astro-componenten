{
  "name": "@geensnor/astro-componenten",
  "version": "2.1.8",
  "description": "Paar prachtige Astro componenten in Geensnor stijl",
  "homepage": "https://github.com/geensnor/astro-componenten#readme",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./de-snor": "./src/de-snor.astro",
    "./kop": "./src/kop.astro",
    "./knoepert": "./src/knoepert.astro",
    "./lijstitem": "./src/lijstitem.astro",
    "./tegel": "./src/tegel.astro",
    "./voettekst": "./src/voettekst.astro"
  },
  "files": [
    "index.ts",
    "src/*",
    "assets/*",
    "fonts/*"
  ],
  "keywords": [
    "astro-component",
    "geensnor"
  ],
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "format": "npx prettier -w src/*",
    "lint": "eslint --ext .ts,.js,.astro src/*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.50.0",
    "eslint-plugin-astro": "^0.29.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.12.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "astro": "^5.14.1"
  },
  "peerDependencies": {
    "esbuild": "^0.25.9"
  }
}
